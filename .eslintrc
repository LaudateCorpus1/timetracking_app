{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "globals": {
    "Templates": true,
    "$": true,
    "jQuery": true,
    "Handlebars": true,
    "_": true,
    "moment": true
  },
  "rules": {
    "no-console": 1,
    "no-unused-vars": 1
  }
}
